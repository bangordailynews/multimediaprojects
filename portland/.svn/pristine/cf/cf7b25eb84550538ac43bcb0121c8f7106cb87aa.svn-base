        <script type="text/javascript">
        $(function() {

            $('#intro').css({
                'width': $(window).width(),
                'height': $(window).height()
            });

            // Big Video intro
            var BV = new $.BigVideo({
                container: $('#intro')
            });
            BV.init();
            if (Modernizr.touch) {
                BV.show('images/priced-out-static-image.png');
            } else {
                BV.show(
                    'http://watchvideo.bangordailynews.com/bdn/2014/06/port_proj_cine_1719549.m4v', {
                        ambient: true
                    }
                );
            }
        });
